doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        link(rel="stylesheet", href="css/style.min.css")
        title Document
    body.ultimate 
        .wrapper  
            include components/header 
            +header 
            main.main 
                include components/intro 
                +compets-intro
                include components/teachers
                +compets-teachers 
                include components/people
                +people
                include components/tutorial
                +internal-tutorial
                include components/diplomas
                +diplomas
                include components/samples 
                +olympiads-samples 
                include components/calc 
                +olympiads-calc
                include components/templates 
                +templates
            include components/footer 
            +footer
        script(src="js/app.min.js") 