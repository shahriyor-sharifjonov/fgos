                .teachers__row 
                    .teachers__item 
                        .teachers__top 
                            .teachers__img 
                                img(src="@img/teachers-img-2.png", alt="" draggable="false")
                            p.teachers__subtitle УДОБНО, ПРОСТО, 24/7
