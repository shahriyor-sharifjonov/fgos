doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        link(rel="stylesheet", href="css/style.min.css")
        title Document
    body.olympiads 
        .wrapper  
            include components/header 
            +header 
            main.main 
                include components/intro 
                +media-intro
                include components/teachers
                +media-teachers 
                include components/accept
                +accept
                include components/people
                +publications-people
                include components/tutorial
                +tutorial
                include components/training
                +olympiads-training
                include components/diplomas
                +publications-diplomas
                include components/patterns
                +patterns 
                include components/templates
                +templates 
                include components/reviews 
                +reviews 
                include components/faq 
                +faq 
            include components/footer 
            +footer
        script(src="js/app.min.js") 